{
  "id": "al2023-tenv",
  "name": "tenv for Amazon Linux 2023",
  "version": "1.0.0",
  "description": "OpenTofu, Terraform, Terragrunt, and Atmos version manager for Amazon Linux 2023",
  "containerEnv": {
    "PATH": "/opt/rye/shims:${PATH}"
  },
  "options": {
    "ryeVersion": {
      "type": "string",
      "description": "rye version to install",
      "proposals": [
        "0.39.0",
        "0.38.0",
        "0.37.0",
        "0.36.0",
        "0.35.0",
        "0.34.0",
        "0.33.0"
      ],
      "default": "latest"
    }
  }
}
